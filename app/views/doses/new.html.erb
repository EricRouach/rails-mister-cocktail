<div class="new-image">
  <h1 class="text-center">New Dose</h1>
  <div class="container", style="border-radius: 4px;">
    <div class="row">
      <div class="col-xs-12 col-sm-6 col-sm-offset-3">
        <div class="new">
          <div class="new-body">
            <%= render 'form', dose: @dose %>
          </div>
          <%= link_to 'Back', cocktail_path(@cocktail), class: "btn btn-warning" %>
        </div>
      </div>
    </div>
  </div>
</div>
